{
  "name": "furious/psr15",
  "description": "PHP 7.4 implementation of PSR-15",
  "license": "MIT",
  "keywords": ["http", "client", "psr15", "request", "php", "php 7.4"],
  "authors": [
    {
      "name": "Arslanov Rasul",
      "email": "rasularslanoov@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "psr/http-server-handler" : "^1.0",
    "psr/http-server-middleware": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.4"
  },
  "provide": {
    "psr/http-server-handler-implementation": "1.0",
    "psr/http-server-middleware-implementation": "1.0"
  },
  "autoload": {
    "psr-4": {
      "Furious\\Psr15\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Furious\\Psr15\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
